config GOOGLE_MODULES
	def_bool y
	select DRM_MIPI_DSI
	select GPIOLIB_IRQCHIP
	select REGMAP_IRQ
	select SND_SOC_COMPRESS
	select VIDEOMODE_HELPERS

config CAIMITO
	bool "Build caimito kernel"
	default y

config SHUSKY
	bool "Build shusky kernel"
	depends on !CAIMITO
	default n
