/dts-v1/;
/plugin/;

#define ESIM_NO_CHIP_ENABLE_PIN
#include "zuma-caimito-common.dtsi"
#include "zuma-caiman-common.dtsi"
#include "google-dts-base/zumapro-phone-gps.dtsi"

/ {
	board_id = <0x40902>;
	board_rev = <0x010002>;
	fragment@boardbase {
		target-path = "/";
		__overlay__ {
			model = "ZUMA CAIMAN Proto 1.0 Broadcom GNSS and MAG3 board based on ZUMA";
			compatible = "google,ZUMA CAIMAN", "google,ZUMA";
		};
	};
};
