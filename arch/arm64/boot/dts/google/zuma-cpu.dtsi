				1622000       820000
				1549000       820000
				1418000       615000
				1328000       615000
				1221000       615000
				1082000       615000
				910000        615000
				721000        615000
				697000        615000
				578000        615000
				402000        324000 >;
		};
		med_bci_perf: dm-table@0005 {
			const-type = <CONSTRAINT_MIN>;
			dm-constraint = <DM_BCI>;
			driver-cal-id = <ACPM_DVFS_CPUCL1>;
			constraint-cal-id = <ACPM_DVFS_BCI>;

				/* CL1          BCI */
			table = < 2600000       840000
				2550000       840000
				2450000       840000
				2352000       840000
				2245000       840000
				2130000       840000
				1999000       840000
				1836000       840000
				1622000       840000
				1549000       840000
