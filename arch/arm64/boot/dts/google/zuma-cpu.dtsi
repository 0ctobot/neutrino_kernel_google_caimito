				1328000       575000
				1221000       575000
				1082000       575000
				910000        575000
				721000        575000
				697000        575000
				578000        575000
				402000        324000 >;
		};
		big_dsu_perf: dm-table@0008 {
			const-type = <CONSTRAINT_MIN>;
			dm-constraint = <DM_DSU>;
			driver-cal-id = <ACPM_DVFS_CPUCL2>;
			constraint-cal-id = <ACPM_DVFS_DSU>;

				/* CL2          DSU */
			table = < 3310000       975000
				3178000       975000
				3015000       975000
